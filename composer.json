{
    "name": "cultuurnet/broadway-amqp",
    "description": "AMQP integration for Broadway library",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "broadway/broadway": "~0.5.2",
        "php-amqplib/php-amqplib": "2.5.*",
        "nicolopignatelli/valueobjects": "~2.0",
        "psr/log": "~1.0",
        "cultuurnet/deserializer": "dev-feature/III-844 as 0.1",
        "2dotstwice/collection": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "phpunit/php-token-stream": "1.4.6",
        "squizlabs/php_codesniffer": "~2.3",
        "phing/phing": "~2.11",
        "satooshi/php-coveralls": "~0.7"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\BroadwayAMQP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\BroadwayAMQP\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
